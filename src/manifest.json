{
  "name": "Jitsi speaker stats exporter",
  "version": "1.1",
  "description": "This extension exports speaker statistics of a jitsi room as a CSV file.",
  "permissions": [
    "activeTab",
    "declarativeContent"
  ],
  "web_accessible_resources": [
    "/injected.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/images/logo16.png",
      "32": "assets/images/logo32.png",
      "48": "assets/images/logo48.png",
      "128": "assets/images/logo128.png"
    }
  },
  "icons": {
    "16": "assets/images/logo16.png",
    "32": "assets/images/logo32.png",
    "48": "assets/images/logo48.png",
    "128": "assets/images/logo128.png"
  },
  "manifest_version": 2
}
